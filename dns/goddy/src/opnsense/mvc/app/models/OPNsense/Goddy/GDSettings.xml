<model>
    <mount>//OPNsense/Goddy/settings</mount>
    <description>
        The OPNsense DynamicDNS mount and model for the Settings-view.
    </description>
    <version>0.2.2</version>
    <items>
        <enabled type="BooleanField">
            <default>1</default>
            <Required>Y</Required>
        </enabled>
        <interface type="InterfaceField">
            <Required>Y</Required>
            <multiple>N</multiple>
            <default>wan</default>
            <filters>
                <enable>/^(?!0).*$/</enable>
            </filters>
        </interface>
        <apikey type="TextField">  
            <default></default>
            <Required>Y</Required>
        </apikey>
        <apisecret type="TextField">
             <default></default>
             <Required>Y</Required>
        </apisecret>
        <atrecord type="BooleanField">
            <default>1</default>
            <Required>Y</Required>
        </atrecord>
    </items>
</model>
